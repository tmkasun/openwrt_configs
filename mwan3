
config rule 'facebook'
	option proto 'all'
	option sticky '1'
	option ipset 'facebook'
	option use_policy 'testp'

config rule 'fbcdn'
	option proto 'all'
	option sticky '1'
	option ipset 'fbcdn'
	option use_policy 'testp'

config rule 'tco'
	option proto 'all'
	option sticky '1'
	option ipset 'tco'
	option use_policy 'testp'

config rule 'twitter'
	option proto 'all'
	option sticky '1'
	option ipset 'twitter'
	option use_policy 'testp'

config rule 'instagram'
	option proto 'all'
	option sticky '0'
	option ipset 'instagram'
	option use_policy 'testp'

config rule 'twimg'
	option proto 'all'
	option sticky '1'
	option ipset 'twimg'
	option use_policy 'testp'

config rule 'all'
	option dest_ip '0.0.0.0/0'
	option dest_port '443,80'
	option proto 'all'
	option sticky '1'
	option use_policy 'all_wans'

config globals 'globals'
	option mmx_mask '0x3F00'
	option rtmon_interval '5'
	option enabled '1'

config policy 'testp'
	list use_member 'testm'
	option last_resort 'unreachable'

config interface 'SLT_FIBER'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list track_ip '1.1.1.1'

config interface 'DIALOG_4G'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list track_ip '1.1.1.1'
	option enabled '1'

config member 'testm'
	option interface 'SLT_FIBER'

config member 'testb'
	option interface 'DIALOG_4G'

config policy 'all_wans'
	option last_resort 'unreachable'
	list use_member 'testb'

