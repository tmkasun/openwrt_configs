config rule 'all'
	option dest_ip '0.0.0.0/0'
	option proto 'all'
	option sticky '1'
	option use_policy 'Dialog4G_Only'

config globals 'globals'
	option mmx_mask '0x3F00'
	option rtmon_interval '5'
	option enabled '1'

config interface 'SLT_FIBER'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list track_ip '1.1.1.1'

config interface 'DIALOG_4G'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list track_ip '1.1.1.1'
	option enabled '1'

config interface 'VPN_Bypass'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list track_ip '1.1.1.1'
	option enabled '1'

config member 'D4G'
	option interface 'DIALOG_4G'
	option metric '1'
	option weight '1'

config policy 'Dialog4G_Only'
	list use_member 'D4G'
	option last_resort 'unreachable'

config member 'SLT'
	option interface 'SLT_FIBER'

config policy 'SLT_Only'
	list use_member 'SLT'
	option last_resort 'unreachable'

